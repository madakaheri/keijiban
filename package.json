{
  "name": "keijiban",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.6",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "ava": "^6.4.1",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "sass": "^1.92.1",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "xo": "^1.2.2"
  },
  "xo": {
    "rules": {
